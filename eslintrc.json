{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
      },
    "extends": ["prettier"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
      
      "ignorePatterns": ["**/node_modules/*", "node_modules/*"],
      
      "plugins": ["@typescript-eslint"],
      "rules": {
        "camelcase": ["error"],
        "indent": ["error", 2],
        "space-before-function-paren": ["error"]
      }
}